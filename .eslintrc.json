{
    "env": {
        "commonjs": true,
        "es2021": true,
        "node": true
    },
    "globals": {
        "test": "readonly",
        "window": "readonly"
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12
    },
    "rules": {
        "indent": ["error", 4],
        "linebreak-style": ["error", "windows"],
        "arrow-body-style":"off",
        "object-shorthand":"off"
    }
}
